#!/bin/sh
#
# Siyi N7 specific board sensors init
#------------------------------------------------------------------------------
board_adc start

# Internal SPI bus ICM-20689
icm20689 -R 2 -s start

# Internal SPI BMI088
bmi088 -A -R 2 -s start
bmi088 -G -R 2 -s start


# internal compass
ist8310 -I -R 10 start

# Baro on internal SPI
ms5611 -s start

ist8310 -X -b 1 -R 10 start
